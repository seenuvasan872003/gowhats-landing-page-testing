{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "buildCommand": "npx vite build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|webp)$)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }